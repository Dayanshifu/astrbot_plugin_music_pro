{
  "api_url": {
    "type": "string",
    "label": "网易云API地址",
    "description": "NeteaseCloudMusicApi 服务地址",
    "default": "http://127.0.0.1:3000"
  },
  "quality": {
    "type": "string",
    "label": "默认音质",
